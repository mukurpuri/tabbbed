{
  "name": "Tabbbed",
  "version": "0.1",
  "manifest_version": 2,


  "description": "Group your tabs for more clearity",
  
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  
  "browser_action": {
    "default_title": "Show TabBoard"
  },

  "permissions": ["tabs","http://*/*", "https://*/*"],

  "icons": {
    "128": "icon.png",
    "256": "icon.png",
    "512": "icon.png"
  }

}
